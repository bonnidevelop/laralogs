{
    "name": "laralogs/laralogs",
    "description": "A beautiful Laravel package for viewing and managing application logs with a modern web interface",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "LaraLogs Team",
            "email": "hello@laralogs.com"
        }
    ],
    "keywords": [
        "laravel",
        "logs",
        "logging",
        "debug",
        "monitoring",
        "web-interface"
    ],
    "homepage": "https://github.com/bonnidevelop/laralogs",
    "support": {
        "issues": "https://github.com/bonnidevelop/laralogs/issues",
        "source": "https://github.com/bonnidevelop/laralogs"
    },
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0",
        "orchestra/testbench": "^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "LaraLogs\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaraLogs\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaraLogs\\LaraLogsServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
